package OCA_ClassObject;
class A {
	int x;
	void m() {
		System.out.println(x++);
	}
}
public class OCA_ClassObject1 {
	public static void main(String[] args) {
		A a=new A();
		System.out.println(a.x);
		a.m();
		A a2=new A();
		a2.m();
		System.out.println(a2.x);
		a2.m();
		System.out.println(a.x);
	}
}
